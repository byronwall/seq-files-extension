{
  "name": "seq-files",
  "displayName": "seq_files",
  "publisher": "none",
  "description": "Syntax highlighting for SEQ files",
  "version": "0.1.1",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:seq"
  ],
  "contributes": {
    "languages": [
      {
        "id": "seq",
        "aliases": [
          "Test Scripts",
          "seq"
        ],
        "extensions": [
          ".seq"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "seq",
        "scopeName": "source.seq",
        "path": "./syntaxes/seq.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.41.0",
    "typescript": "^3.7.5",
    "vsce": "^1.96.1",
    "vscode-test": "^1.3.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "dependencies": {}
}
